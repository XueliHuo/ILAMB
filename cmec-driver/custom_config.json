    "runtime": {
      "ILAMB": 2.6
    }
  },
  "varlist": {
    "any": {
      "long_name": "any variable",
      "units": "any",
      "frequency": "mon"
    }
  },
  "obslist": {
    "any": {
      "version": "any",
      "long_name": "any observation",
      "description": "Any obs available in CMIP configuration"
    }
  },
  "coordinates": {
    "max_time_range": "year",
    "min_time_range": "mon"
  },
  "default_parameters": {
    "confrontations": ["Surface Upward SW Radiation","Albedo"]
  }
}
